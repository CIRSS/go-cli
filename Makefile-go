
build:
	go build ./...

# disable test result caching using: -count 1
# disable parallel execution of tests using: -p 1 
test:
	go test ./... -count 1 -p 1

clean:
	go clean ./...
	chmod -R +w ${GOPATH}
	rm -rf ${GOPATH}/*

#package-code:           # Package custom artifacts for distribution.
#	go build -o ./.build ./... 

#install-code:           # Install custom code.
#	go install ./...   
